@using HoaxBuzzer.Web.Helper
@model HoaxBuzzer.Web.Models.AfterVoteScreenModel

@{
    Layout = null;
}

@if (Model.Article.articleIsTrue)
{
    <h1 class="stamp stamp--true">the article was REAL!</h1>
}
else
{
    <h1 class="stamp stamp--false">the article was FAKE!</h1>
}

<div stamp="@(Model.VoteStatistics.articleIsTrue ? "true":"false")">
    @Html.Partial("Partial/Article", Model)
</div>

<pre>
@Model.VoteStatistics.DumpJson()
</pre>